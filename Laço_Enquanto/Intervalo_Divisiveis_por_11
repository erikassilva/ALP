//Escreva um algoritmo que gere o números de 1000 a 1999 e escreva aqueles que dividido por 11 dão resto igual a 5.

#include <iostream>

using namespace std;

int main (int argc, char** argv){
	
	int i=1000;
	
	cout << "Numeros divisiveis por 11 cujo resto eh = 5\n";
	while (i <= 1999){
		cout << i << "\n";
		if (i % 11 == 5){
			cout << i << " -Dividido por 11 resta 5\n";
		}
		i ++;
	}
	
	return 0;
}
